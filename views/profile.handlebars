<html class="profile">
<div class="row">
  <div class="col-auto text-center">
    <h2 class="profilehead">Welcome, {{name}}!</h2>
  </div>
</div>

<div class="row new-entry">
  <div class="card card-rounded col-md-4 text-center">
    <div class="card-body">
      <button class="btn btn-sm btn-danger create-entry-btn"></button>
    </div>
    <h3 class="card-header">Create New Entry</h3>
  </div>

  <div class="card card-rounded col-md-4 text-center entry">
    <div class="card-body">
      <button class="btn btn-sm btn-danger show-entry-btn"></button>
    </div>
    <h3 class="card-header">Browse Entries</h3>
  </div>

  <div class="card card-rounded col-md-4 text-center">
    <div class="card-body">
      <button class="btn btn-sm btn-danger show-library-btn"></button>
    </div>
    <h3 class="card-header">Browse Photos</h3>
  </div> 
</div>
<script src="./js/profile.js"></script>
<script src="./js/entrylist.js"></script>
<script src="./js/library.js"></script>
</html>
