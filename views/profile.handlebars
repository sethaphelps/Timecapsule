<div class="row">
  <div class="col-auto text-center">
    <h2>Welcome, {{name}}!</h2>
  </div>
</div>

<div class="row new-entry">
  <div class="card card-rounded col-md-4 text-center">
    <h3 class="card-header">Create new entry</h3>
    <div class="card-body">
      <button class="btn btn-sm btn-danger create-entry-btn">Create New</button>
    </div>
  </div>

  <div class="card card-rounded col-md-4 text-center entry">
    <h3 class="card-header">Browse Entries</h3>
    <div class="card-body">
      <button class="btn btn-sm btn-danger show-entry-btn">See More</button>
    </div>
  </div>

  <div class="card card-rounded col-md-4 text-center library">
    <h3 class="card-header">Browse Photos</h3>
    <div class="card-body">
      <button class="btn btn-sm btn-danger show-library-btn">See your library</button>
    </div>
  </div> 
</div>



<script src="./js/profile.js"></script>
<script src="./js/entrylist.js"></script>
<script src="./js/library.js"></script>
