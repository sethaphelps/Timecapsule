<div class="row">
  <div class="col-auto">
    <h2>Welcome, {{name}}!</h2>
  </div>
</div>

<div class="col-md-6 new-entry">
    <h3>Create a new note!</h3>
    <div class="row mb-2">
      <div class="col-md-4">
        <button class="btn btn-sm btn-danger create-entry-btn">Create New</button>
      </div>
    </div>
  </div>
</div>

<div class="col-md-6 entry">
    <h3>See all notes!</h3>
    <div class="row mb-2">
      <div class="col-md-4">
        <button class="btn btn-sm btn-danger show-list-btn">See all notes</button>
      </div>
    </div>
  </div>
</div>

<script src="./js/profile.js"></script>
<script src="./js/entrylist.js"></script>
