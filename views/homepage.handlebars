<html class="homepage">
<div class="container container-fluid mt-5">
  <h2>Log in to track your daily memories, notes, & photos</h2>
  <div class="col-md-6">
    <div class="col-auto text-center">
    </div>
  </div>
  <form class="form login-form">
    <div class="form-group">
      <label for="email-login">email:</label>
      <input class="form-input" type="text" id="email-login" />
    </div>
    <div class="form-group">
      <label for="password-login">password:</label>
      <input class="form-input" type="password" id="password-login" />
    </div>
    <div>      
      <button class="btn btn-danger" type="submit">Log In</button>
      <button class="btn btn-danger sign-btn" type="submit">Sign up</button>
      <p class="text-danger log-again"> </p>   
    </div>
  </form>
</div>

<script src="./js/login.js"></script>
</html>