<html class="create">
<div class="col-md-6">
  <button class="btn btn-sm btn-danger"><a href="/">Back to Profile</a></button>
  <h3>Write a new entry!</h3>
  
  <form class="form image-form" action="" method="post" enctype="multipart/form-data" onsubmit="AJAXSubmit(this); return false;">
    <div class="form-group">
    </div>
    <div class="form-group">
      <label class="form-group">Upload a photo:</label>
      <p>
        <input type="hidden" name="upload_preset" value="new_one" />
      </p>
      <p>
        <label>Select your photo: <input type="file" name="file" /></label>
      </p>
    </div>
    <button type="submit" value="Submit" class="btn btn-sm btn-danger create-btn">Upload</button>
    <p class="text-danger create-response"></p>
    <img width="200px" height="200px" id="uploaded"/>
    <div id="results"></div>
  </form>

  <form class="form new-entry-form" action="" method="post">
        <div class="form-group">
      <label for="entry-name">Title:</label>
      <input class="form-input" type="text" id="entry-name" name="entry-name" />
    </div>
    <div class="form-group">
      <label for="entry-desc">Description:</label>
      <textarea class="form-input" id="entry-desc" name="entry-desc"></textarea>
    </div>
      <button type="submit" value="Submit" class="btn btn-sm btn-danger create-btn">Create</button>
    <p class="text-danger create-response"></p>
    <img id="uploaded"/>
    <div id="results"></div>
  </form>
</div>
<script src="./js/create.js"></script>
<script src="./js/index.js"></script>
</html>